<?xml version="1.0" encoding="UTF-8"?>


<mbeans-descriptors>

	<mbean name="Hello" description="the hello bean" domain="Study"
		group="helloGroup" type="com.lm.common.jmx.demo.xml.Hello" >
		
		<attribute name="name" description="a name attribute"
			type="java.lang.String" writeable="true" />
			
		<operation name="printHello"
			description="a operation to print hello" impact="INFO"
			returnType="String" />
			
		<operation name="printHello"
			description="a operation to print hello" impact="ACTION"
			returnType="String">
			
			<parameter name="whoName" description="who's name."
				type="java.lang.String" />
		</operation>
		
	</mbean>
	
</mbeans-descriptors>